    "js/cors/jquery.xdr-transport.min.js",
    "js/vendor/jquery.ui.widget.min.js",
    "css/jquery.fileupload-noscript.min.css",
    "css/jquery.fileupload-ui-noscript.min.css",
    "css/jquery.fileupload-ui.min.css",
    "css/jquery.fileupload.min.css"
